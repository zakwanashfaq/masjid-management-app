{
  "name": "masjid-serverless-api",
  "description": "Api",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.188.0",
    "@aws-sdk/lib-dynamodb": "^3.188.0",
    "@types/node": "^20.11.17"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "aws-sdk-client-mock": "^2.0.0",
    "jest": "^29.2.1"
  },
  "scripts": {
    "start": "sam build && sam local start-api",
    "start-hot-reload": "nodemon --exec \"sam build && sam local start-api\"",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/__tests__/**/*.mjs?(x)",
      "**/?(*.)+(spec|test).mjs?(x)"
    ],
    "moduleFileExtensions": [
      "mjs",
      "js"
    ]
  },
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}